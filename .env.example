# Google APIs
# Enable these APIs in Google Cloud Console:
# - Maps JavaScript API
# - Places API
# - Maps Static API
# - Solar API
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
GOOGLE_SOLAR_API_KEY=your_google_solar_api_key_here

# Supabase
# Get these from your Supabase project settings
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Email (Resend)
# Get this from https://resend.com/api-keys
RESEND_API_KEY=re_your_resend_api_key_here
ROOFER_NOTIFICATION_EMAIL=contractor@example.com

# App Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_PARTNER_NAME="Your Roofing Partner"
NEXT_PUBLIC_PARTNER_SCHEDULING_URL=https://calendly.com/your-partner

# Pricing Configuration (per square, in dollars)
# These are customizable per partner/region
PRICE_ECONOMY_PER_SQUARE=450
PRICE_STANDARD_PER_SQUARE=550
PRICE_PREMIUM_PER_SQUARE=700
